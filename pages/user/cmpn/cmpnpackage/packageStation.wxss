page{
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #434242;
}

.list{
  margin:30rpx;
  background: #FFFFFF;
  border-radius: 20rpx;
  padding:30rpx;
  box-sizing: border-box;
}

.list_title{
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
}

.list_text{
  padding:16rpx 0 14rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #EBEBEB;
}

.list_addr{
  margin-left:5rpx;
  font-size:22rpx;
  color: #999999;
}